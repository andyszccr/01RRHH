@model IEnumerable<RRHH.Models.TipoDeduccione>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DeduccionNombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeduccionPorcentaje)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DeduccionNombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DeduccionPorcentaje)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.TipoDeduccionId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.TipoDeduccionId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.TipoDeduccionId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
